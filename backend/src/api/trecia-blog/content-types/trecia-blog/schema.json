{
  "kind": "collectionType",
  "collectionName": "trecia_blogs",
  "info": {
    "singularName": "trecia-blog",
    "pluralName": "trecia-blogs",
    "displayName": "TreciaBlog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true,
      "default": "TreciaKS' Blog"
    },
    "content": {
      "type": "richtext",
      "required": false
    },
    "blogImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "trecia_blogs"
    }
  }
}
